        "#calar cada uno con plt\n",
        "import matplotlib.pyplot as plt\n",
        "n = len(mask)\n",
        "\n",
        "\"\"\"\n",
        "for i in range(1,n):\n",
        "    if n%i==0:\n",
        "        plt.imshow(mask.reshape( (i, n//i)))\n",
        "        plt.show()\n",
        "        print( f\"( {i}, {n//i} )\" ) #para mostrar la cadena de imagenes que me sirve\n",
        "\"\"\"\n",
        "\n",
        "resultado = mask.reshape ( (127,-1) )\n",
